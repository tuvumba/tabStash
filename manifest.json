{
    "manifest_version": 2,
    "name": "Tab Stash",
    "version": "1.0",
    "description": "Save and restore sessions of open tabs.",
    "icons": {
        "128": "iconBook.png"
    },
    "permissions": [
      "tabs",
      "storage"
    ],
    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "iconBook.png"
  }
  }
  